function setCookie(e, o) {
    document.cookie = e + "=" + escape(o)
}

function getCookie(e) {
    return document.cookie.length > 0 && (cookie_start = document.cookie.indexOf(e + "="), -1 != cookie_start) ? (cookie_start = cookie_start + e.length + 1, cookie_end = document.cookie.indexOf(";", cookie_start), -1 == cookie_end && (cookie_end = document.cookie.length), unescape(document.cookie.substring(cookie_start, cookie_end))) : ""
}

function eTouchNavPadding() {
    menuButtonHeight = $("#menuButton").height(), $(".resizeRespond").width() < 3 ? $(".hNavOuterWrapper, .vNavOuterWrapper").css("padding-top", menuButtonHeight) : $(".hNavOuterWrapper, .vNavOuterWrapper").css("padding-top", "")
}

function eSubBody() {
    windowHeight = $(window).height(), menuButtonHeight = $("#menuButton").height(), hNavHeight = $(".hNavOuterWrapper").length > 0 ? $(".hNavOuterWrapper").height() : "", vNavWidth = $(".vNavOuterWrapper").length > 0 ? $(".vNavOuterWrapper").width() : "", $(".resizeRespond").width() < 3 ? ($(".hSubBody, .vSubBody").css("padding-top", menuButtonHeight), $(".vSubBody").css("padding-left", "")) : ($(".hSubBody").css("padding-top", hNavHeight), $(".vSubBody").css("padding-top", ""), $(".vSubBody").css("padding-left", vNavWidth)), 2 == $(".resizeRespond").width() ? $(".vhubBody, .vSubBody").css("height", windowHeight - menuButtonHeight) : $(".resizeRespond").width() > 2 ? $(".hSubBody, vSubBody").css("height", windowHeight - hNavHeight) : $(".hSubBody, .vSubBody").css("height", "")
}

function eResponsiveImages() {
    $("img:not(#eliteLightBox img)").each(function() {
        $(this).parent().width() < $(this).attr("width") ? $(this).css("width", "100%") : $(this).css("width", "")
    })
}

function eStickyNav() {
    topOfWindow = $(window).scrollTop(), $(".resizeRespond").width() > 2 && topOfWindow > navPos ? $(".vNavOuterWrapper, .hNavOuterWrapper").addClass("fixToTop") : $(".vNavOuterWrapper, .hNavOuterWrapper").removeClass("fixToTop")
}

